import java.util.Scanner;
public class ReverseNumber {

	public static void main(String[] args) {
		int x,temp,rev=0; ;
		System.out.println("Enter the no. :");
		Scanner sc= new Scanner(System.in);
		x= sc.nextInt();
		
		while(x!=0)
		{
			temp=x%10;
			x=x/10;
			rev=(rev*10)+temp;
		}
		System.out.println("After Reverse the no. is "+rev);
	}

}
